<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jinhyun.a10thmenu">

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jinhyun.a10thmenu.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jinhyun.a10thmenu.MainActivity"/>
        <activity android:name="com.jinhyun.a10thmenu.LoginActivity"/>
        <activity android:name="com.jinhyun.a10thmenu.OwnerActivity"/>
    </application>

</manifest>